{
    "preset": "laravel",
    "rules": {
        "simplified_null_return": false,
        "phpdoc_no_access": false,
        "phpdoc_no_package": false,
        "phpdoc_separation": false,
        "phpdoc_summary": false,
        "phpdoc_to_comment": false,
        "concat_space": {
            "spacing": "one"
        }
    }
}